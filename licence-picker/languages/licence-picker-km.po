#. Translators: 1 is plugin name, 2 is version number
#: admin/class-lp-admin.php:54
#, php-format
msgid "%1$s %2$s not activated"
msgstr "%1$s %2$s មិន​សកម្ម"

#. Translators: 1 is current WordPress version, 2 is required version
#: admin/class-lp-admin.php:56
#, php-format
msgid "You are running WordPress version %1$s. This plugin requires version %2$s or higher, and has been deactivated! Please upgrade WordPress and try again."
msgstr "អ្នក​កំពុង​ដំណើរការ WordPress កំណែ %1$s ។ កម្មវិធី​ជំនួយ​នេះ​ទាមទារ​កំណែ %2$s ឬ​ខ្ពស់ជាង, ហើយ​ត្រូវ​បាន​ធ្វើឲ្យ​អសកម្ម​ទៀត! សូម​ធ្វើ​បច្ចុប្បន្នភាព WordPress រួច​ព្យាយាម​ម្ដងទៀត។"

#: admin/class-lp-admin.php:57
msgid "Back to WordPress admin"
msgstr "ត្រឡប់​ទៅ​ទំព័រ​គ្រប់គ្រង WordPress"

